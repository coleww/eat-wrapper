{
  "name": "eat-wrapper",
  "description": "Wrapper functions for the Edinburgh Associative Thesaurus",
  "version": "1.0.1",
  "main": "index.js",
  "scripts": {
    "build": "browserify www/demo.js -o www/bundle.js",
    "test": "standard && node test.js",
    "watch": "watchify www/demo.js -o www/bundle.js --debug --verbose",
    "postInstall": "curl https://raw.githubusercontent.com/dariusk/ea-thesaurus/master/ea-thesaurus.json >> eat.json"
  },
  "devDependencies": {
    "browserify": "*",
    "standard": "*",
    "tape": "*",
    "watchify": "*"
  },
  "license": "MIT",
  "author": "Cole Willsea <coleww@gmail.com> (http://colewillsea.com/)",
  "dependencies": {
    "browserify": "^11.0.1",
    "standard": "^4.5.4",
    "tape": "^4.0.1",
    "json-file": "^0.1.0",
    "watchify": "^3.3.1",
    "corpora-project": "^0.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coleww/eat-wrapper.git"
  },
  "keywords": [
    "words",
    "associations",
    "poetry"
  ],
  "bugs": {
    "url": "https://github.com/coleww/eat-wrapper/issues"
  },
  "homepage": "https://github.com/coleww/eat-wrapper"
}
